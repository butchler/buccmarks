{
  "name": "Buccmarks",
  "version": "1.0",
  "description": "Visual bookmarks for Chrome",
  "background_page": "background.html",
  "browser_action": {
     "default_icon": "buccmarks.png",
     "default_title": "See your bookmarks :)"
   },
  "icons": {
   "128": "buccmarks128.png",
   "48": "buccmarks48.png"
  },
  "permissions": ["tabs", "*://*/*", "bookmarks", "unlimitedStorage", "chrome://favicon/"]
}
